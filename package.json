{
  "name": "my-component-library",
  "author": "Kiwamu ISHII",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@8.11.0",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "engines": {
    "node": "20",
    "pnpm": "8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  },
  "scripts": {
    "clean": "rimraf build dist",
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\" && eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "preinstall": "typesync || :",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "tailwind-config-viewer": "tailwind-config-viewer --open --config ./tailwind.config.cjs"
  },
  "dependencies": {
    "lint-staged": "^15.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.5",
    "@storybook/addon-interactions": "^7.6.5",
    "@storybook/addon-links": "^7.6.5",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/blocks": "^7.6.5",
    "@storybook/react": "^7.6.5",
    "@storybook/react-vite": "^7.6.5",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "1.0.4",
    "autoprefixer": "10.4.16",
    "browserslist": "^4.22.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-access": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "16.4.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unused-imports": "^3.0.0",
    "glob": "^10.3.10",
    "happy-dom": "^12.10.3",
    "husky": "8.0.3",
    "json": "11.0.0",
    "open-color": "^1.9.1",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "postcss-nesting": "^12.0.2",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.1.1",
    "prop-types": "15.8.1",
    "storybook": "^7.6.5",
    "stylelint": "^16.0.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-no-unsupported-browser-features": "^7.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "stylelint-use-nesting": "^4.1.0",
    "tailwind-config-viewer": "^1.7.3",
    "tailwindcss": "3.3.7",
    "tsc-alias": "^1.8.8",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "1.0.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
